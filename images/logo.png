  <path d="M80,25 Q90,35 80,45 Q70,55 80,65" stroke="#CD7F32" stroke-width="3" fill="none"/>
  <path d="M75,30 Q85,40 75,50" stroke="#CD7F32" stroke-width="2" fill="none"/>
  
  <!-- Hotel name and tagline -->
